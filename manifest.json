{
  "name": "StopSound - Cancel video's background music",
  "description": "You can now remove live background music from the videos you react to in order to avoid copyright concerns.",
  "version": "1.3",
  "manifest_version": 2,
  "permissions": ["storage", "activeTab"],
  "browser_action": {
    "default_popup": "popup-wrapper.html",
    "default_icon": {
      "16": "/images/logo16.png",
      "32": "/images/logo32.png",
      "48": "/images/logo48.png",
      "128": "/images/logo128.png"
    }
  },
  "web_accessible_resources": [
    "popup.html"
  ],
  "icons": {
    "16": "/images/logo16.png",
    "32": "/images/logo32.png",
    "48": "/images/logo48.png",
    "128": "/images/logo128.png"
  }
}
